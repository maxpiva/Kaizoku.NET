{
  "StorageFolder": "",
  "BridgeFolder": "mihon",
  "ThumbCacheFolder": "thumbs",
  "TempFolder": "",
  "CacheCheckInDays": 7,
  "FirstTimeSettings": {
    "PreferredLanguages": [ "en" ],
    "MihonRepositories": [ "https://raw.githubusercontent.com/keiyoushi/extensions/repo" ],
    "NumberOfSimultaneousDownloads": 10,
    "NumberOfSimultaneousSearches": 10,
    "NumberOfSimultaneousDownloadsPerProvider": 3,
    "PerTitleUpdateSchedule": "02:00:00",
    "PerSourceUpdateSchedule": "00:30:00",
    "ExtensionsCheckForUpdateSchedule": "01:00:00",
    "ChapterDownloadFailRetries": 144,
    "ChapterDownloadFailRetryTime": "00:30:00",
    "CategorizedFolders": true,
    "Categories": [ "Manga", "Manhwa", "Manhua", "Comic", "Other" ],
    "FlareSolverrEnabled": false,
    "FlareSolverrUrl": "http://127.0.0.1:8191",
    "FlareSolverrTimeout": "00:00:30",
    "FlareSolverrSessionTtl": "00:15:00",
    "FlareSolverrAsResponseFallback": false,
    "SocksProxyEnabled": false,
    "SocksProxyVersion": 5,
    "SocksProxyHost": "",
    "SocksProxyPort": 0,
    "SocksProxyUsername":"",
    "SocksProxyPassword":""
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=kaizoku.db"
  },
  "Kestrel": {
    "Ports": {
      "Debug": 9833,
      "Release": 9833
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "outputTemplate": "[{App} {Timestamp:HH:mm:ss} {Level:u3} {PascalClassName,-20}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
